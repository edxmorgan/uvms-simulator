            <box size="0.05 0.05 0.05"/>
            </geometry>
            <material name="payload_blue">
            <color rgba="0 0 1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.025" rpy="0 0 0"/>
