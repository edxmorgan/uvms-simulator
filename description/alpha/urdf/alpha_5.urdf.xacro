            <box size="0.05 0.05 0.05"/>
            </geometry>
        </collision>
        </link> -->

        <!-- <joint name="${name}_payload_joint" type="fixed">
        <parent link="${name}_jaws_base_link"/>
        <child  link="${name}_payload"/>
        <origin xyz="0 0 0.04" rpy="0 0 0"/>
        </joint> -->

    </xacro:macro>
</robot>